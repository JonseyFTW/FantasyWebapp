# Database
DATABASE_URL="postgresql://username:password@localhost:5432/fantasy_football_db"
DATABASE_URL_TEST="postgresql://username:password@localhost:5432/fantasy_football_test"

# Server Configuration
PORT=4000
NODE_ENV="development"

# Authentication
JWT_SECRET="your-jwt-secret-here"
JWT_EXPIRES_IN="7d"

# External APIs
SLEEPER_API_BASE_URL="https://api.sleeper.app/v1"
